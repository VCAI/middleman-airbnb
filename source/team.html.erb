<div id="about">
  <div class="about-content">
    <h1>This template was built with <i class="fa fa-heart"></i> by</h1>
      <ul class = "list-inline text-center">
        <% data.team.each do |user| %>
        <li>
          <%= image_tag "https://kitt.lewagon.com/placeholder/users/#{user.github_nickname}", class: "img-circle" %>
          <h3 class= "user-desc"><%= user.name %></h3>
          <% link_to "https://www.github.com/#{user.github_nickname}", target: "_blank" do %>
          <i class= "fa fa-github glogo"></i><%= "#{user.github_nickname}" %>
          <%end%>
        </li>
        <% end %>
      </ul>
  </div>
</div>
